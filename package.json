{
  "name": "node-red-contrib-aws",
  "version": "0.1.2",
  "description": "A collection of Node-RED nodes for AWS.  Includes nodes for DynamoDB, EC2, Kinesis, Lambda, SNS, SQS and S3",
  "dependencies": {
    "aws-sdk": "^2.0.15",
    "minimatch": "2.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daniel-t/node-red-contrib-aws"
  },
  "license": "Apache",
  "keywords": [
    "node-red",
    "aws",
    "ec2",
    "kinesis",
    "lamdba",
    "s3",
    "sqs",
    "sns",
    "dynamodb"
  ],
  "node-red": {
    "nodes": {
      "amazon ec2": "ec2.js",
      "amazon kinesis": "kinesis.js",
      "amazon lambda": "lambda.js",
      "amazon s3": "s3.js",
      "amazon dynamodb": "dynamodb.js",
      "amason sns": "sns.js",
      "amason sqs": "sqs.js",
      "amazon config": "aws-config.js"
    }
  },
